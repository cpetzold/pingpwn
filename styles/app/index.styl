#content
  margin-top: 50px

#standings
  .position
    width: 16px
    text-align: center

#addPlayer,
#addGame,
#modal-backdrop
  display: none

.ac_results
  z-index: 11001
  background: #fff
  border: 1px solid #ccc
  border-top: none
  border-radius: 3px
  ul
    margin: 0
    padding: 0
  li
    padding: 3px
    &.ac_odd
      background: #fafafa
    &.ac_over
      background: #eee
